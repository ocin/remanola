{% extends "devices/effect/effect-nopatch.remotemap" %}
{% set deviceman="Propellerheads" -%}
{% set devicescope="MClass Compressor" -%}
{% set devicename="MClass Compressor" -%}
{% set shortname="mclasscompressor" -%}
{% block groups %}
Define Group	Page	Main	Env	Index	Internal
{% endblock groups %}
{% block main %}
// Index
Map	PageName		"Index"			Index
Map	Button 1-1		Page=Main			Index
Map	Button 2-8		Page=Env			Index
Map	Button C7		Page=Internal			Index
// Main
Map	PageName		"Main"			Main
{{ rm.include_menus(curpage="Main") -}}
Map	UDVButton 1-1_2-1		Enabled			Main
Map	Button 7-1		Sidechain Active			Main
Map	Button 8-1		Sidechain Solo			Main
Map	Meter 2		Peak Meter			Main
Map	Knob V3		Input Gain			Main
Map	Fader 4		Threshold			Main
Map	Button 1-5		Soft Knee			Main
Map	Fader 6		Ratio			Main
Map	Meter 7		Gain Meter			Main
Map	Knob V8		Output Gain			Main
// Env
Map	PageName		"Env"			Env
{{ rm.include_menus(curpage="Env") -}}
Map	Fader 4		Attack			Env
Map	Fader 5		Release			Env
Map	Button 1-6		Adapt			Env
{% endblock main %}
