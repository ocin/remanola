{% set oscpage="Osc" %}
{{ rm.include_menus(curpage="Eng"+engnum|string+"Osc",mainmenu_mini="common", submenu="osc", oscpage="Osc", engnum=engnum) -}}
Map	Button 1-2		Osc{{ engnum }} On			Eng{{ engnum }}Osc	// template="BRedOnOff"
Map	UDVButton 1-3_2-3		Osc{{ engnum }} Oct			Eng{{ engnum }}Osc	// template="UDFreq"
