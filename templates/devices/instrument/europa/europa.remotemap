// Main
Map	PageName		"Main"			Main
{{ rm.include_menus(curpage="Main",mainmenu_mini="common") -}}
Map	Fader 8		Master Volume			Main	// template="FGain"
// Perf
Map	PageName		"Perf"			Perf
{{ rm.include_menus(curpage="Perf",mainmenu_mini="common") -}}
